@model List<Human>


@if (Model != null)
{
    <table class="table">
        <tr>
            <th>Id</th>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Age</th>
            <th>IsSick</th>
            <th>Gender</th>
        </tr>
        @foreach (var human in Model)
        {
            <tr>
                <td>@human.Id</td>
                <td>@human.FirstName</td>
                <td>@human.LastName</td>
                <td>@human.Age</td>
                <td>@human.IsSick</td>
                <td>@human.Gender</td>
            </tr>
        }
    </table>
}


