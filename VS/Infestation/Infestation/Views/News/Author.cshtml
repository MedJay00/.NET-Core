@model List<News>

@section AdditionalInfo{
    <table class="table">
        <tr>
            <th>AuthorId</th>
            <th>Id</th>
            <th>Title</th>
            <th>Text</th>
            <th>Fake</th>
        </tr>
        @foreach (var news in Model)
        {
    <tr>
        <td>@news.Author.Id</td>
        <td>@news.Id</td>
        <td>@news.Title</td>
        <td>@news.Text</td>
        <td>@news.IsFake</td>
    </tr>
        }
    </table>
}